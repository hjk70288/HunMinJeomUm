<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ROOT</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
      <servlet-name>MemberServlet</servlet-name> 
      <servlet-class>controller.MemberServlet</servlet-class>
</servlet>
	<servlet-mapping>
      <servlet-name>MemberServlet</servlet-name>
      <url-pattern>/MemberServlet</url-pattern> 
</servlet-mapping>
  <servlet>
      <servlet-name>OcrServlet</servlet-name> 
      <servlet-class>controller.OcrServlet</servlet-class>
</servlet>

	<servlet-mapping>
      <servlet-name>OcrServlet</servlet-name>
      <url-pattern>/OcrServlet</url-pattern> 
</servlet-mapping>
  <servlet>
      <servlet-name>PostServlet</servlet-name> 
      <servlet-class>controller.PostServlet</servlet-class>
</servlet>

	<servlet-mapping>
      <servlet-name>PostServlet</servlet-name>
      <url-pattern>/PostServlet</url-pattern> 
</servlet-mapping>
</web-app>